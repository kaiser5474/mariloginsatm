        "XbasicFunctionDeclarations": "function editarPreguntasAuditoria2Parte as v (e as p)\n\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim argsP as sql::Arguments\n\t\n\tdim Id_Auditoria as n\n\tId_Auditoria = e.DataSubmitted.Id_Auditoria\n\t\n\tdim Id_Proceso as n\n\tId_Proceso = e.DataSubmitted.Id_Proceso\n\t\n\targsP.Add(\"Id_Auditoria\", Id_Auditoria)\n\targsP.Add(\"Id_Proceso\", Id_Proceso)\n\t\n\tdim rs as sql::ResultSet\n\tsql = <<%sql%\n\tSELECT   Auditoria_2Parte_Pregunta_Entidad.Cumple, Auditoria_2Parte_Pregunta.Puntos, Auditoria_2Parte_Pregunta_Entidad.Cantidad, \n\t\t\t Auditoria_2Parte_Pregunta_Entidad.Id_Pregunta_Auditoria, Auditoria_2Parte_Pregunta.Condicion, Auditoria_2Parte_Pregunta.ValorCondicion, \n\t\t\t Auditoria_2Parte_Pregunta.Id_Proceso\n\tFROM     Auditoria_2Parte_Pregunta_Entidad INNER JOIN\n             Auditoria_2Parte_Pregunta ON dbo.Auditoria_2Parte_Pregunta_Entidad.Id_Pregunta_Auditoria = dbo.Auditoria_2Parte_Pregunta.Id_Pregunta_Auditoria\n\tWHERE    Auditoria_2Parte_Pregunta_Entidad.Id_Auditoria = :Id_Auditoria\n\t%sql%\n\t\n\tcn.Execute(sql, argsP)\t\n\trs = cn.ResultSet\n\t\n\tdim puntacionRiesgoEntidad as n\n\tpuntacionRiesgoEntidad = 100\n\t\n\tdim Id_Pregunta_Auditoria as n\n\tdim cantidad as n\n\tdim condicion as c\n\tdim valorCondicion as n\n\tdim Id_Proceso as n\n\tdim Cumple as l\n\tdim Puntos as n\n\tdim esBasc as l\t\n\t\n\tdim frecuenciaCS as n\n\tdim kapital as n\n\tdim valorCS as n\n\tdim periodoPago as n\n\tdim porcientoCompraMRG as n\n\t\n\twhile rs.NextRow()\n\t\tesBasc = .F.\n\t\tCumple = rs.Data(1)\t\n\t\tPuntos = rs.Data(2)\n\t\tcantidad = rs.Data(3)\n\t\tId_Pregunta_Auditoria = rs.Data(4)\n\t\tcondicion = rs.Data(5)\n\t\tvalorCondicion = rs.Data(6)\n\t\tId_Proceso = rs.Data(7)\n\t\t\t\n\t\t\tif Id_Proceso = 3  then\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 45 .OR. Id_Pregunta_Auditoria = 46 .OR. Id_Pregunta_Auditoria = 47) .AND. Cumple = .T. then\t\t\t\t\t\t\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 49) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tkapital = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 50) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tvalorCS = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 51) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tfrecuenciaCS = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 52) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tperiodoPago = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 53) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tporcientoCompraMRG = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\tend if\n\t\t\t\n\t\t\tif Id_Proceso = 2  then\n\t\t\t\t\tif Id_Pregunta_Auditoria = 23 .AND. Cumple = .T. then\t\t\t\t\t\t\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\t\n\t\t\t\t\t\t\tesBasc = .T.\t\t\t\t\t\n\t\t\t\t\t\telse\n\t\t\t\t\t\t\tif esBasc = .F. .AND. Cumple = .T. .AND. Id_Pregunta_Auditoria <> 33 .AND. Id_Pregunta_Auditoria <> 39 then\n\t\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\n\t\t\t\t\t\t\tend if\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\tend if\n\t\t\t\n\t\t\tif Id_Proceso = 4 then\n\t\t\t\t\tif condicion = \"Mayor\"  then\n\t\t\t\t\t\tif cantidad >= valorCondicion .AND. cantidad > 0 then\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\n\t\t\t\t\t\tend if\n\t\t\t\t\t\telse\n\t\t\t\t\t\t\tif cantidad <= valorCondicion .AND. cantidad > 0 then\n\t\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\n\t\t\t\t\t\t\tend if\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\tend if\n\t\t\t\t\n\t\t\tif Cumple = .t. .AND. Id_Proceso = 1 then\t\t\t\t\n\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\t\t\t\t\t\t\t\n\t\t\tend if\t\n\t\t\t\n\t\t\t\n\t\t\t\n\tend while\n\t\n\t\t\tif kapital > 0 .AND. frecuenciaCS > 0 .AND. porcientoCompraMRG > 0 then\n\t\t\t\t\t\tcalculoCapacidadOperativa = (((valorCS * (periodoPago/frecuenciaCS))/(kapital*(porcientoCompraMRG/100))))\n\t\t\t\t\t\tif calculoCapacidadOperativa <= 10 then\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - 7\n\t\t\t\t\t\tend if\n\t\t\tend if\n\n\n\tsql = <<%sql%\n\tUPDATE Auditoria_2Parte Set valorRiesgo = :puntacionRiesgoEntidad WHERE Id_Auditoria = :Id_Auditoria\n\t%sql%\n\t\n\targsP.Add(\"puntacionRiesgoEntidad\", puntacionRiesgoEntidad)\n\n\tdim flagResult as l\n\tflag = cn.Execute(sql, argsP)\t\n\t\ncn.Close()  \n                         \nend function    \n\n\nfunction insertarPreguntasAuditoria2Parte as v (e as p)\n\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim argsP as sql::Arguments\n\t\n\tdim Id_Auditoria as n\n\tId_Auditoria = e._currentRowDataNew.Id_Auditoria 'e.lastIdentityValue \n\t\n\tdim Id_Entidad as n\n\tId_Entidad = e._currentRowDataNew.Id_Entidad 'e.DataSubmitted.Id_Entidad\n\t\n\tdim Id_Pregunta_Entidad as n\n\tId_Pregunta_Entidad = e._currentRowDataNew.Id_Pregunta_Entidad\n\t\n\tdim Id_Pregunta_Auditoria as n\n\tId_Pregunta_Auditoria = e._currentRowDataNew.Id_Pregunta_Auditoria\n\t\n\targsP.Add(\"Id_Entidad\", Id_Entidad)\n\targsP.Add(\"Id_Pregunta_Auditoria\", Id_Pregunta_Auditoria)\n\t\n\tdim rs as sql::ResultSet\n\tsql = <<%sql%\n\tSELECT  Id_ClasificacionEntidad\n\tFROM    N_Entidad\n\tWHERE   Id_Entidad = :Id_Entidad\n\t%sql%\t\n\t\n\tcn.Execute(sql, argsP)\t\n\trs = cn.ResultSet\n\n\tsql = <<%sql%\n\tSELECT  Descripcion, Id_Pregunta_Auditoria, Id_Proceso\n\tFROM    Auditoria_2Parte_Pregunta\n\tWHERE Id_Pregunta_Auditoria = :Id_Pregunta_Auditoria\n\t%sql%\n\n\tdim flagResult as l\n\tflag = cn.Execute(sql, argsP)\n\tdim txt as c\n\t\n\ttxt = cn.CallResult.text\n\t\n\trs = cn.ResultSet\n\tdim descripcionPregunta as c\n\tdim Id_Pregunta_Auditoria as n\n\tdim Id_Proceso as n\n\t\n\targsP.Add(\"Pregunta\", \"\")\n\targsP.Add(\"Id_Auditoria\", Id_Auditoria)\n\targsP.Add(\"Id_Proceso\", 0)\n\targsP.Add(\"Cumple\", \"False\")\n\targsP.Add(\"Observacion\", \"Pendiente\")\n\t\n\tdim puntuacionRiesgoEntidad as n\n\tpuntuacionRiesgoEntidad = 100\n\t\n\tdim args2 as sql::Arguments\n\targs2.Add(\"Id_Entidad\", Id_Entidad)\n\t\n\tdim fechaValidez as d\n\tdim observacion as c\n\tdim DetalleCertificacionSeguros as c\n\t\n\tdim rs2 as sql::ResultSet\n\t\t\n\twhile rs.NextRow()\n\t\targsP.Set(\"Observacion\", \"Pendiente\")\n\t\targsP.Set(\"Cumple\", \"False\")\n\t\tdescripcionPregunta = rs.Data(1)\n\t\tId_Pregunta_Auditoria = rs.Data(2)\n\t\tId_Proceso = rs.Data(3)\n\t\targsP.Set(\"Pregunta\", descripcionPregunta)\n\t\targsP.Set(\"Id_Pregunta_Auditoria\", Id_Pregunta_Auditoria)\n\t\targsP.Set(\"Id_Proceso\", Id_Proceso)\t\t\t\n\t\t\n\t\targs2.Set(\"Id_Entidad\", Id_Entidad)\n\t\t\n\t\tif Id_Pregunta_Auditoria = 31 then\n\t\t\tsql = <<%sql%\t\t\t\n\t\t\tSELECT   MAX(FechaValidez) AS FechaValidez\n\t\t\tFROM     Entidad_Documentos\n\t\t\tWHERE    (Id_Entidad = :Id_Entidad) AND (Id_ClasificacionEntidadDocumentos = 24)\n\t\t\tGROUP BY Id_ClasificacionEntidadDocumentos\n\t\t\t%sql%\n\t\t\t\n\t\t\tcn.Execute(sql, args2)\n\t\t\t\n\t\t\trs2 = cn.ResultSet\n\t\t\t\n\t\t\twhile rs2.NextRow()\n\t\t\t\tfechaValidez = rs2.Data(1)\n\t\t\t\tif fechaValidez - now() > 0 then\n\t\t\t\t\tpuntuacionRiesgoEntidad = puntuacionRiesgoEntidad - 1\n\t\t\t\t\targsP.Set(\"Cumple\", \"True\")\n\t\t\t\t\targsP.Set(\"Observacion\", \"Tiene vigencia de \" + convert_type(fechaValidez - now(), \"C\") + \" dias\")\n\t\t\t\t\telse\n\t\t\t\t\t\targsP.Set(\"Cumple\", \"False\")\n\t\t\t\t\t\targsP.Set(\"Observacion\", \"El documento esta vencido\")\n\t\t\t\tend if\n\t\t\tend while\t\t\t\t\n\t\tend if\n\t\t\n\t\tif Id_Pregunta_Auditoria = 32 then\n\t\t\tsql = <<%sql%\t\t\t\n\t\t\tSELECT   MAX(FechaValidez) AS FechaValidez\n\t\t\