        "XbasicFunctionDeclarations": "function enviarEmailContactos as c (e as p)'esta funcion es para enviar los reportes dado el tipo de transportacion multimodal\n\t'Codigo para cargar los correos\n\tdim Id_RegistroAsistencia as n\n\tId_RegistroAsistencia = e._currentRowDataNew.Id_RegistroAsistencia\n\t\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim rs as sql::ResultSet\t\n\t\n\t'** Aqui comienzo a buscar el tipo de evento\n\tdim args2 as sql::Arguments\n\t\n\tdim Id_TipoEvento as n\n\tId_TipoEvento = e._currentRowDataNew.Id_TipoEvento\n\t\n\tsql = <<%sql%\n\tSELECT DISTINCT Descripcion, Id_TipoEvento \n\tFROM N_TipoEvento_RegistroAsistencia \n\tWHERE Id_TipoEvento = :Id_TipoEvento\n\t%sql%\t\n\targs2.Add(\"Id_TipoEvento\", Id_TipoEvento)\n\t\n\tcn.Execute(sql, args2)\n\trs = cn.ResultSet\n\t\n\tdim TipoEvento as c\n\t\n\twhile rs.NextRow()\n\t\tTipoEvento = rs.Data(1)\n\tend while\n\t'** Aqui termino de buscar el tipo de evento\n\t\n\t'** aqui comienzo a buscar el contenido del evento\n\tdim Id_ContenidosEvento as n\n\tId_ContenidosEvento = e._currentRowDataNew.Id_ContenidoEvento\n\t\n\tsql = <<%sql%\n\tSELECT DISTINCT Descripcion, Id_ContenidosEvento \n\tFROM N_ContenidosEventos\n\tWHERE Id_TipoEvento_RegistroAsistencia = :Id_TipoEvento AND Id_ContenidosEvento = :Id_ContenidosEvento\n\t%sql%\t\n\targs2.Add(\"Id_TipoEvento\", Id_TipoEvento)\n\targs2.Add(\"Id_ContenidosEvento\", Id_ContenidosEvento)\n\t\n\tcn.Execute(sql, args2)\n\trs = cn.ResultSet\n\t\n\tdim ContenidosEvento as c\n\t\n\twhile rs.NextRow()\n\t\tContenidosEvento = rs.Data(1)\n\tend while\n\t'** aqui termino a buscar el contenido del evento\n\t\n\t'** aqui comienzo a buscar la descripcion del evento\n\tdim Id_DescripcionEvento as n\n\tId_DescripcionEvento = e._currentRowDataNew.Id_ContenidoEventoDescripcion\n\t\n\tsql = <<%sql%\n\tSELECT DISTINCT Descripcion, Id_DescripcionEvento \n\tFROM N_ContenidosEventos_Descripciones \n\tWHERE Id_DescripcionEvento = :Id_DescripcionEvento\n\t%sql%\t\n\targs2.Add(\"Id_DescripcionEvento\", Id_DescripcionEvento)\n\t\n\t\n\tcn.Execute(sql, args2)\n\trs = cn.ResultSet\n\t\n\tdim DescripcionEvento as c\n\t\n\twhile rs.NextRow()\n\t\tDescripcionEvento = rs.Data(1)\n\tend while\n\t'** aqui termino de buscar la descripcion del evento\n\t\n\t'** aqui comienzo a buscar el lugar del evento\n\tdim Id_LugarEvento as n\n\tId_LugarEvento = e._currentRowDataNew.Lugar\n\t\n\tsql = <<%sql%\n\tSELECT  DISTINCT (((N_Entidad.Organizacion + ' (') + N_Entidad.Referencia) + ')') AS Expr1, \tId_Entidad  \n\tFROM N_Entidad   \n\tWHERE Id_Entidad = :Id_LugarEvento\n\t%sql%\t\n\targs2.Add(\"Id_LugarEvento\", Id_LugarEvento)\n\t\n\t\n\tcn.Execute(sql, args2)\n\trs = cn.ResultSet\n\t\n\tdim LugarEvento as c\n\t\n\twhile rs.NextRow()\n\t\tLugarEvento = rs.Data(1)\n\tend while\n\t'** aqui termino de buscar el lugar del evento\n\t\n\t'** aqui estoy haciendo el listado de Invitados\n\t\n\tdim invitados as c\n\tdim argsP as sql::Arguments\n\targsP.Add(\"Id_RegistroAsistencia\", Id_RegistroAsistencia)\t\n\t\n\tsql = <<%sql%\n\tSELECT  Registro_Eventos.Id_RegistroAsistencia, N_Contactos.Nombre + ' ' + N_Contactos.Apellidos AS NombreCompleto\n\tFROM    Registro_Eventos INNER JOIN\n            Registro_Evento_Contactos ON dbo.Registro_Eventos.Id_RegistroAsistencia = dbo.Registro_Evento_Contactos.Id_RegistroEvento INNER JOIN\n            N_Contactos ON dbo.Registro_Evento_Contactos.Id_Contacto = dbo.N_Contactos.Id_Contacto\n\tWHERE   (Registro_Eventos.Id_RegistroAsistencia = :Id_RegistroAsistencia)\n\t%sql%\t\n\tcn.Execute(sql, argsP)\n\trs = cn.ResultSet\n\t\n\tdim nombre as c\n\tdim invitados as c\n\t\n\twhile rs.NextRow() \n\t\tnombre = rs.Data(2)\n\t\tnombre = strtran(nombre, \"á\", \"&aacute;\")\n\t\tnombre = strtran(nombre, \"é\", \"&eacute;\")\n\t\tnombre = strtran(nombre, \"í\", \"&iacute;\")\n\t\tnombre = strtran(nombre, \"ó\", \"&oacute;\")\n\t\tnombre = strtran(nombre, \"ú\", \"&uacute;\")\n\t\tif invitados = 