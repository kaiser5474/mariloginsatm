nction copiarElemento as c (e as p)\n\tdim json as c  'aqui estoy cargando el json que obtengo de seleccionar los viajes que van a ser prefacturados \n\tjson = e.__additionalData\n\tdim p as p\n\tp = json_parse(json)\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\tdim flagResult as l \n\tdim id_Categoria as n\n\tdim id_tipoCategoria as n\n\tid_Categoria = p[1].ID_CATEGORIA\n\tid_tipoCategoria = p[1].ID_TIPOCATEGORIA\n\tdim id_categoria2 as n\n\t'dim sql as c\n\tdim args2 as sql::Arguments\t\n\targs2.Add(\"id_Categoria\", id_tipoCategoria)\n\tdim sqlStatement as c \n\tsqlStatement = <<%sql%\n\tselect MIN(Id_Categoria) Id_Categoria from EvaluacionDesempeno_Categoria where Id_TipoCategoria = :id_Categoria\n\t%sql%\n\t\n\tflagResult = cn.Execute(sqlStatement, args2)\nif flagResult = .f. then \n\tui_msg_box(\"Error\",cn.CallResult.text)\n\tcn.close()\n\tend \nend if \ndim rs2 as sql::resultset\nrs2 = cn.ResultSet\nid_categoria2 = rs2.Data(1) \ndim args3 as sql::Arguments\t\nargs3.Add(\"id_Categoria\", id_Categoria2)\n\t'Execute a query on a SQL database\n'DIM a connection variable\n'DIM cn as SQL::Connection\nflagResult = cn.open(\"::Name::Nibble\")\nif flagResult = .f. then \n\tui_msg_box(\"Error\",\"Could not connect to database. Error reported was: \" + crlf() + cn.CallResult.text)\n\tend \nend if\n'Specify if you are using Portable SQL syntax, or not\ncn.PortableSQLEnabled = .t. \ndim sqlStatement2 as c \nsqlStatement2 = <<%sql%\nSELECT Descripcion  FROM EvaluacionDesempeno_Elementos  WHERE Id_Categoria = :id_Categoria\n%sql%\n'Execute the Query\nflagResult = cn.Execute(sqlStatement2, args3)\nif flagResult = .f. then \n\tui_msg_box(\"Error\",cn.CallResult.text)\n\tcn.close()\n\tend \nend if \ndim rs as sql::resultset\nrs = cn.ResultSet\n'The sql_resultset_preview() function opens a window showing a table with the results\n'of your query. Show a maximum of 1,000 rows of data\n'Note that the connection object is passed into the sql_resultset_preview() fu