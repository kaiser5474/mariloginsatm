        "XbasicFunctionDeclarations": "function envioComprobante as c (e as p)\n\t\n\tdim id as n\n\tid = e._currentRowDataNew.Id_RegistroPagados\n\t\n\tdim creador as c\n\tcreador = e._currentRowDataNew.CREADOR\n\t\n\tdim b as b \n\tdim a as c\n\tdim nombreDoc as c\n\t\n\t'Execute a query on a SQL database\n\t'DIM a connection variable\n\tDIM cn as SQL::Connection\n\tdim flagResult as l \n\tflagResult = cn.open(\"::Name::Nibble\")\n\tif flagResult = .f. then \n\t\tui_msg_box(\"Error\",\"Could not connect to database. Error reported was: \" + crlf() + cn.CallResult.text)\n\t\tenvioComprobante = \"alert('No se pudo conectar a la BD, intente nuevamente.')\"\n\t\tend \n\tend if\n\n\t'Specify if you are using Portable SQL syntax, or not\n\t'cn.PortableSQLEnabled = .t. \n\t\t\n\tdim args as sql::Arguments\n\targs.Add(\"id\", id)\n\t\n\tdim noFactura as c\n\tnoFactura = \"\"'e._currentRowDataNew.NOFACTURA\n\t\n\tdim IP as c\n\tIP = \"\"\n\t\t\n\tdim sqlStatement as c \n\tsqlStatement = <<%sql%\n\tSELECT \tRegistroProveedores.NoFactura, RegistroProveedores.Id_RegistroProv \n\tFROM (RegistroProveedores RegistroProveedores\n\t\t LEFT OUTER JOIN N_Entidad N_Entidad\n\t\t\t ON  RegistroProveedores.Id_EntidadProv = N_Entidad.Id_Entidad \n\t\t LEFT OUTER JOIN RegistroProveedores_Pagado RegistroProveedores_Pagado\n\t\t\t ON  RegistroProveedores.Id_RegistroProv = RegistroProveedores_Pagado.Id_RegistroProveedor )  \n\tWHERE RegistroProveedores_Pagado.Id_RegistroPagados = :id\n\t%sql%\n\t\n\t'Execute the Query\n\tflagResult = cn.Execute(sqlStatement, args)\n\tif flagResult = .f. then \n\t\tui_msg_box(\"Error\",cn.CallResult.text)\n\t\tcn.close()\n\t\tend\n\t\telse\n\t\tdim rs20 as sql::resultset\n\t\trs20 = cn.ResultSet\n\t\n\t\twhile rs20.NextRow()\n\t\t\tif noFactura = \"\" then\n\t\t\t\tnoFactura = rs20.Data(1)\n\t\t\t\telse\n\t\t\t\t\tnoFactura = noFactura + \", \" + rs20.Data(1)\n\t\t\tend if\n\t\t\tif IP = \"\" then\n\t\t\t\tIP = rs20.Data(2)\n\t\t\t\telse \n\t\t\t\t\tIP = IP + \", \" + rs20.Data(2)\n\t\t\tend if\n\t\t\t\n\t\tend while \n\tend if \t\t\n\t\n\tsqlStatement = <<%sql%\n\tSELECT Documento, Archivo, Extension  \n\tFROM RegistroProveedores_Pagados_Documentos \n\tWHERE Id_RegistroPagados = :id\n\t%sql%\n\t\n\t'Execute the Query\n\tflagResult = cn.Execute(sqlStatement, args)\n\tif flagResult = .f. then \n\t\tui_msg_box(\"Error\",cn.CallResult.text)\n\t\tcn.close()\n\t\tend\n\t\telse \n\t\t\tdim rs as sql::resultset\n\t\t\trs = cn.ResultSet\n\t\t\n\t\t\twhile rs.NextRow()\n\t\t\t\tb = rs.Data(1)\n\t\t\t\tnombreDoc = rs.Data(2)\n\t\t\tend while\n\tend if \t\n\n\tsqlStatement = <<%sql%\n\tSELECT Contactos_Correos.Correo\n\tFROM   RegistroProveedores_Pagados_ContactoEmail INNER JOIN\n\t       Contactos_Correos ON RegistroProveedores_Pagados_ContactoEmail.Id_Correo = Contactos_Correos.Id_ContactoCorreo\n\tWHERE RegistroProveedores_Pagados_ContactoEmail.Id_RegistroPagados = :id\n\t%sql%\n\t\n\tflagResult = cn.Execute(sqlStatement, args)\n\tif flagResult = .f. then \n\t\tui_msg_box(\"Error\",cn.CallResult.text)\n\t\tcn.close()\n\t\tend \n\tend if \n\t\n\tdim rsCorreo as sql::resultset\n\trsCorreo = cn.ResultSet\n\ndim correosProveedor as c\ndim correoContacto as c\n\nwhile rsCorreo.NextRow()\n\tcorreoContacto = rsCorreo.Data(1)\n\tcorreosProveedor = correoContacto + \", \" + correosProveedor\nend while\n\nif correosProveedor = null_value() then\n\t\n\tenvioComprobante = \"alert('No hay correos del proveedor asociados.')\"\n\t\n\telse\n\n\tif b <> null_value() then\n\t\tsqlStatement = <<%sql%\t\n\t\tSELECT  N_FichaTrabajadores.EmailAddress1\n\t\tFROM    RegistroProveedores AS RegistroProveedores LEFT OUTER JOIN\n\t\t        RegistroProveedores_Pagado AS RegistroProveedores_Pagado ON RegistroProveedores.Id_RegistroProv = RegistroProveedores_Pagado.Id_RegistroProveedor INNER JOIN\n\t\t        WebSecurityUsers ON RegistroProveedores.creador = WebSecurityUsers.Userid INNER JOIN\n\t\t        N_FichaTrabajadores ON WebSecurityUsers.Id_Usuario = N_FichaTrabajadores.Id_Usuario\n\t\tWHERE        (RegistroProveedores_Pagado.Id_RegistroPagados = :id)\n\t\tGROUP BY N_FichaTrabajadores.EmailAddress1\n\t\t%sql%\n\t\t\n'\t\tdim args2 as sql::Arguments\n'\t\targs2.Add(\"creador\", creador)\n\t\t\n\t\tflagResult = cn.Execute(sqlStatement, args)\n\t\tif flagResult = .f. then \n\t\t\tui_msg_box(\"Error\",cn.CallResult.text)\n\t\t\tcn.close()\n\t\t\tend \n\t\tend if \n\t\tdim rs2 as sql::resultset\n\t\trs2 = cn.ResultSet\n\t\t\n\t\tdim emailCreador as c\n\t\t\n\t\twhile rs2.NextRow()\n\t\t\temailCreador = rs2.Data(1)\t\n\t\tend while\n\t\t\n\t\t'session.GetDataFromFile(b, a)\n\t\tdim size as n \n\t\t\tsize = b.size()\n\t\t\tif size = 0 then \n\t\t\t\temailreport = \"alert('Error -- report was not printed');\"\n\t\t\t\texit function \n\t\t\tend if \n\t\t\t\n\t\t\tfile.From_blob(nombreDoc,b)\n\t\t\t\n\t\t\tdim pProfile as p\n\t\t\tdim msg as c \n\t\t\tdim pSocket as p\n\t\t\tpSocket.timeout = 5000\n\t\t\tpProfile.client_hostname = \"secure.emailsrvr.com\"\n\t\t\tpProfile.smtp_port = 465\n\t\t\tpProfile.smtp_username = \"reportes@mariloginsatm.com\"\n\t\t\tpProfile.smtp_password = \"DIJ017^eN;H2Jckb|ktK\"\n\t\t\tdim secureMode as c = \"SSL\"\n\t\t\tdim SHARED pMessage as p\n\t\t\tdim pMessage.message as C\n\t\t\tdim pMessage.html_message as C \n\t\t\n\t\t\tdim pMessage._to as C = correosProveedor + \", \" + emailCreador + \", raquel.velasco@mariloginsa.net, marcos.marcel@mariloginsa.net, evelin.velasco@mariloginsa.net, jarvismiranda728096@gmail.com, cristina.michelle@mariloginsa.net\"\n\t\t\tdim pMessage.subject as C = \"IP: \" + IP + \" PAGADO(S)\"\n\t\t\tdim pMessage.attachments as C \n\t\t\tdim pMessage.cc as C\n\t\t\tdim pMessage.bcc as C\n\t\t\tdim pMessage.lRelated as L\n\t\t\tdim pMessage.ReturnReceipt as L\n\t\t\tdim pMessage.from as C = \"reportes@mariloginsatm.com\"\n\t\t\tdim pMessage.from_alias as C = \"MariloginsaTM\"\n\t\t\tdim flagOpen as l \n\t\t\tdim lSilent as l = .f.\t\n\t\t\t\n\t\t\t\n\t\t\t\n\t\t\tdim pMessage.message as C\n\t\t\tpMessage.message = <<%txt% \nEstimados señores: \n\nReciban un cordial saludo de parte de LOGISTICA MARITIMA INTEGRAL S.A., el presente correo tiene como fin informar el pago de la factura(s) %txt% + noFactura + \". \" + <<%txt% \n\nComo respaldo se adjunta el correspondiente comprobante de pago en formato pdf. \n\nEn caso de solicitar datos adicionales:\n\nCristina Quinapallo <cristina.michelle@mariloginsa.net>\n\n<mariloginsa@mariloginsa.net> o <mariloginsatm@mariloginsatm.com>                          \t\t\n\nPuede seguirnos en nuestras redes sociales:\nhttps://www.youtube.com/channel/UCY2Gl1pMBqnf3K8Cv8EjQ8g\nhttps://www.twitter.com/Mariloginsa_Ec\nhttps://www.linkedin.com/in/mariloginsa\nhttps://www.facebook.com/LogisticaMaritimaIntegral\n_________________________________________________________\nEl contenido de este mensaje es privilegiado, confidencial y para uso exclusivo de las personas destinatarias de este mensaje asi como por otros especificamente autorizados a recibirlo. Si usted recibiese esta comunicacion por error, por favor tome las medidas apropiadas para destruir el mensaje y/o contactar al remitente.\n%txt%\n\t\t\t\n\t\t\tpMessage.attachments = nombreDoc\t\t\t\n\t\t\t\n\t\t\tflagOpen = email_smtp_open(pSocket,pProfile.client_hostname,pProfile.smtp_port,pProfile.smtp_username, pProfile.smtp_password, secureMode)\n\t\t\tif flagOpen then \n\t\t\t\tdim autowrap as l = .t.\n\t\t\t\tdim flagSent as l\n\t\t\t\tflagSent = email_smtp_send(pMessage, pSocket, (.NOT. lSilent), autowrap)\n\t\t\t\tif flagSent = .f.\n\t\t\t\t\t'msg = \"Los reportes no fueron enviados. Error reportado:\" + pSocket.buffer\n\t\t\t\telse\n\t\t\t\t\t\n\t\t\t\tend if \n\t\t\temail_smtp_close(pSocket)\n\t\t\telse\n\t\t\t'msg = \"Los reportes no fueron enviados. Error reportado: No se puedo conectar via SMTP server: \"+pProfile.client_hostname + crlf()+ pSocket.buffer\n\t\t\tend if \n\t\t\t\n\t\t\t\n\t\t\ta5_file_remove(nombreDoc)\n\t\t\t\n'\t\t\tDIM args2 as sql::arguments\n'\t\t\t\t\n'\t\t\targs2.set(\"New__usuarioPago\",session.Userid)\n'\t\t\targs2.set(\"New__fechaPago\",now())\n'\t\t\targs2.set(\"Id_RegistroProv\",id)\n\t\t\t\n\t\t\tdim msg as c \n\t\t\t\n'\t\t\tdim sqlUpdateStatement as c \n'\t\t\tsqlUpdateStatement = <<%txt%\n'\t\t\tUPDATE RegistroProveedores SET usuarioPago = :New__usuarioPago, fechaPago = :New__fechaPago  \n'\t\t\tWHERE Id_RegistroProv = :Id_RegistroProv\n'\t\t\t%txt%\n'\t\t\t\n'\t\t\tdim flag as l \n'\t\t\tflag = cn.Execute(sqlUpdateStatement,args2)\n'\t\t\tif flag = .f. then \n'\t\t\t\tui_msg_box(\"Error\",\"Record was not updated. Error reported was: \" + crlf(2) + cn.CallResult.text,UI_STOP_SYMBOL)\n'\t\t\telse\n'\t\t\t\tif cn.AffectedRows() > 0 then \n'\t\t\t\t\tui_msg_box(\"Notice\", \"\" + cn.AffectedRows() + \" record(s) updated.\",UI_INFORMATION_SYMBOL)\n'\t\t\t\telse\n'\t\t\t\t\tui_msg_box(\"Error\",\"Record was not updated.\" ,UI_STOP_SYMBOL)\n'\t\t\t\tend if \n'\t\t\tend if \n\t\t\t\n\t\t\t\t\n\t\t\t'Table has an auto-increment field. Get the value that was assigned to this field and store it in\n\t\t\t'a variable.\t\n'\t\t\tdim LastIdentityValue as a\n'\t\t\tif cn.AffectedRows() = 1 then \n'\t\t\t\tLastIdentityValue = cn.callResult.LastInsertedIdentity\n'\t\t\tend if \n\t\t\n\t\t\tenvioComprobante = \"alert('El comprobante de pago ha sido enviado con exito.')\"\n\t\t\t\n\t\t\telse\t\n\t\t\tenvioComprobante = \"alert('No hay comprobante de pago subido.')\"\n\tend if\nend if\ncn.close()\n\nend function",
