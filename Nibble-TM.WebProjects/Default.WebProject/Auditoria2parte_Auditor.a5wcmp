_smtp_send(pMessage, pSocket, (.NOT. lSilent), autowrap)\n\t\tif flagSent = .f.\n\t\t\tmsg = \"El correo no fue enviado. Error reportado:\" + pSocket.buffer\n\t\telse\n\t\t\tmsg = \"El correo fue enviado.\"\t\t\t\n\t\tend if \n\temail_smtp_close(pSocket)\n\telse\n\tmsg = \"Los reportes no fueron enviados. Error reportado: No se puedo conectar via SMTP server: \"+pProfile.client_hostname + crlf()+ pSocket.buffer\n\tend if \n\t\n\telse\n\tmsg = \"Para poder enviar el correo es necesario que la fecha y hora este insertada.\"\nend if\n\t\n\tenviarEmailNotificacion = \"alert('\" + js_escape(msg) + \"');\"\n                  \nend function   \n\nfunction enviarEmailResultados as c (e as p)\n\t\t\t\n\tdim id_contacto as n\n\tid_contacto = e._currentRowDataNew.Responsable\n\t\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql5 as c\n\tsql5 = <<%sql%\n\tSELECT   WebSecurityUsers.Userid, N_FichaTrabajadores.EmailAddress1\n\tFROM     WebSecurityUsers INNER JOIN\n             N_FichaTrabajadores ON WebSecurityUsers.Id_Usuario = N_FichaTrabajadores.Id_Usuario\n    WHERE    WebSecurityUsers.Userid = :Userid\n\t%sql%\n\t\n\tdim Userid as c\t\n\tUserid = session.Userid\t\n\t\n\tdim args5 as sql::Arguments\n\targs5.Add(\"Userid\", Userid)\t\n\t\n\tcn.Execute(sql5, args5)\n\t\n\tdim rs5 as sql::ResultSet\n\trs5 = cn.ResultSet\n\t\n\tdim correoCreador as c\n\t\n\twhile rs5.NextRow()\n\t\tcorreoCreador = rs5.Data(2)\n\tend while\t\n\t\n\tdim sql as c\n\tsql = <<%sql%\n\tSELECT  ((Nombre + ' ') + Apellidos) AS Nombre\n\tFROM N_Contactos    \n\tWHERE Id_Contacto = :id_contacto\n\t%sql%\t\n\tdim argsP as sql::Arguments\n\targsP.Add(\"id_contacto\", id_contacto)\n\t\n\tcn.Execute(sql, argsP)\t\n\t\n\tdim rs as sql::ResultSet\n\trs = cn.ResultSet\n\t\n\tdim nombreContacto as c\n\t\n\twhile rs.NextRow() \n\t\tnombreContacto = rs.Data(1)\n\tend while\t\t\n\t\n\tdim Id_CorreoResponsable as n\n\tId_CorreoResponsable = e._currentRowDataNew.Id_CorreoResponsable\n\t\n\targs