et(\"Id_Entidad\", Id_Entidad)\n\t\t\n\t\tif Id_Pregunta_Auditoria = 1 then\n\t\t\tsql = <<%sql%\t\t\t\n\t\t\tSELECT   MAX(FechaValidez) AS FechaValidez\n\t\t\tFROM     Entidad_Documentos\n\t\t\tWHERE    (Id_Entidad = :Id_Entidad) AND (Id_ClasificacionEntidadDocumentos = 14)\n\t\t\tGROUP BY Id_ClasificacionEntidadDocumentos\n\t\t\t%sql%\n\t\t\t\n\t\t\tcn.Execute(sql, args2)\n\t\t\t\n\t\t\trs2 = cn.ResultSet\n\t\t\t\n\t\t\twhile rs2.NextRow()\n\t\t\t\tfechaValidez = rs2.Data(1)\n\t\t\t\tif fechaValidez - now() > 0 then\n\t\t\t\t\tpuntuacionRiesgoEntidad = puntuacionRiesgoEntidad - 1\n\t\t\t\t\targsP.Set(\"Cumple\", \"True\")\n\t\t\t\t\targsP.Set(\"Observacion\", \"Tiene vigencia de \" + convert_type(fechaValidez - now(), \"C\") + \" dias\")\n\t\t\t\t\telse\n\t\t\t\t\t\targsP.Set(\"Cumple\", \"False\")\n\t\t\t\t\t\targsP.Set(\"Observacion\", \"El documento esta vencido\")\n\t\t\t\tend if\n\t\t\tend while\t\t\t\t\n\t\tend if\n\t\t\n\t\tif Id_Pregunta_Auditoria = 2 then\n\t\t\tsql = <<%sql%\t\t\t\n\t\t\tSELECT   MAX(FechaValidez) AS FechaValidez\n\t\t\tFROM     Entidad_Documentos\n\t\t\tWHERE    (Id_Entidad = :Id_Entidad) AND (Id_ClasificacionEntidadDocumentos = 5)\n\t\t\tGROUP BY Id_ClasificacionEntidadDocumentos\n\t\t\t%sql%\n\t\t\tcn.Execute(sql, args2)\n\t\t\t\n\t\t\trs2 = cn.ResultSet\n\t\t\t\n\t\t\twhile rs2.NextRow()\n\t\t\t\tfechaValidez = rs2.Data(1)\n\t\t\t\tif fechaValidez - now() > 0 then\n\t\t\t\t\tpuntuacionRiesgoEntidad = puntuacionRiesgoEntidad - 1\n\t\t\t\t\targsP.Set(\"Cumple\", \"True\")\n\t\t\t\t\targsP.Set(\"Observacion\", \"Tiene vigencia de \" + convert_type(fechaValidez - now(), \"C\") + \" dias\")\n\t\t\t\t\telse\n\t\t\t\t\t\targsP.Set(\"Cumple\", \"False\")\n\t\t\t\t\t\targsP.Set(\"Observacion\", \"El documento esta vencido\")\n\t\t\t\tend if\t\t\t\t\n\t\t\tend while\t\t\t\t\n\t\tend if\n\t\t\n\t\t'¿Tiene RUC actualizado con actividad declarada conforme a la prestación del servicio?\n\t\tif Id_Pregunta_Auditoria = 7 then\n\t\t\tsql = <<%sql%\t\