        "XbasicFunctionDeclarations": "function editarFechaEnvioNotificacion as v (e as p)\n\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim argsP as sql::Arguments\n\t\n\tdim Id_Auditoria as n\n\tId_Auditoria = e._currentRowDataNew.Id_Auditoria\n\t\n\tdim user as c\n\tuser = session.Userid\n\t\n\targsP.Add(\"Id_Auditoria\", Id_Auditoria)\n\targsP.Add(\"fecha\", now())\n\targsP.Add(\"user\", user)\n\t\n\tsql = <<%sql%\n\tUPDATE Auditoria_2Parte SET envioNotificacion = 'True', fechaEnvioNotificacion = :fecha, usuarioEnvio = :user\n\tWHERE Id_Auditoria = :Id_Auditoria\n\t%sql%\n\t\n\tcn.Execute(sql, argsP)\t\n\t\n\tcn.Close()\t\n\t\nend function\t\n\nfunction editarFechaEnvioResultados as v (e as p)\n\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim argsP as sql::Arguments\n\t\n\tdim Id_Auditoria as n\n\tId_Auditoria = e._currentRowDataNew.Id_Auditoria\n\t\n\tdim user as c\n\tuser = session.Userid\n\t\n\targsP.Add(\"Id_Auditoria\", Id_Auditoria)\n\targsP.Add(\"fecha\", now())\n\targsP.Add(\"user\", user)\n\t\n\tsql = <<%sql%\n\tUPDATE Auditoria_2Parte SET envioResultados = 'True', fechaEnvioResultado = :fecha, usuarioEnvioResultado = :user\n\tWHERE Id_Auditoria = :Id_Auditoria\n\t%sql%\n\t\n\tcn.Execute(sql, argsP)\t\n\t\n\tcn.Close()\t\n\t\nend function\n\nfunction editarPreguntasAuditoria2Parte as v (e as p)\n\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim argsP as sql::Arguments\n\t\n\tdim Id_Auditoria as n\n\tId_Auditoria = e._currentRowDataNew.Id_Auditoria 'e.DataSubmitted.Id_Auditoria\n\t\n\t'dim Id_Proceso as n\n\t'Id_Proceso = e._currentRowDataNew.Id_Proceso 'DataSubmitted.Id_Proceso\n\t\n\targsP.Add(\"Id_Auditoria\", Id_Auditoria)\n\t'argsP.Add(\"Id_Proceso\", Id_Proceso)\n\t\n\tdim rs as sql::ResultSet\n\tsql = <<%sql%\n\tSELECT   Auditoria_2Parte_Pregunta_Entidad.Cumple, Auditoria_2Parte_Pregunta.Puntos, Auditoria_2Parte_Pregunta_Entidad.Cantidad, \n\t\t\t Auditoria_2Parte_Pregunta_Entidad.Id_Pregunta_Auditoria, Auditoria_2Parte_Pregunta.Condicion, Auditoria_2Parte_Pregunta.ValorCondicion, \n\t\t\t Auditoria_2Parte_Pregunta.Id_Proceso\n\tFROM     Auditoria_2Parte_Pregunta_Entidad INNER JOIN\n             Auditoria_2Parte_Pregunta ON dbo.Auditoria_2Parte_Pregunta_Entidad.Id_Pregunta_Auditoria = dbo.Auditoria_2Parte_Pregunta.Id_Pregunta_Auditoria\n\tWHERE    Auditoria_2Parte_Pregunta_Entidad.Id_Auditoria = :Id_Auditoria\n\t%sql%\n\t\n\tcn.Execute(sql, argsP)\t\n\trs = cn.ResultSet\n\t\n\tdim puntacionRiesgoEntidad as n\n\tpuntacionRiesgoEntidad = 100\n\t\n\tdim Id_Pregunta_Auditoria as n\n\tdim cantidad as n\n\tdim condicion as c\n\tdim valorCondicion as n\n\tdim Id_Proceso as n\n\tdim Cumple as l\n\tdim Puntos as n\n\tdim esBasc as l\t\n\t\n\tdim frecuenciaCS as n\n\tdim kapital as n\n\tdim valorCS as n\n\tdim periodoPago as n\n\tdim porcientoCompraMRG as n\n\t\n\twhile rs.NextRow()\n\t\tesBasc = .F.\n\t\tCumple = rs.Data(1)\t\n\t\tPuntos = rs.Data(2)\n\t\tcantidad = rs.Data(3)\n\t\tId_Pregunta_Auditoria = rs.Data(4)\n\t\tcondicion = rs.Data(5)\n\t\tvalorCondicion = rs.Data(6)\n\t\tId_Proceso = rs.Data(7)\n\t\t\t\n\t\t\tif Id_Proceso = 3  then 'Capacidad Operativa\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 45 .OR. Id_Pregunta_Auditoria = 46 .OR. Id_Pregunta_Auditoria = 47) .AND. Cumple = .T. then\t\t\t\t\t\t\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\t\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 49) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tkapital = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 50) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tvalorCS = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 51) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tfrecuenciaCS = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 52) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tperiodoPago = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\tif (Id_Pregunta_Auditoria = 53) then\t\t\t\t\t\t\n\t\t\t\t\t\t\tporcientoCompraMRG = cantidad\t\t\t\t\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\t\t\t\n\t\t\tend if\n\t\t\t\n\t\t\tif Id_Proceso = 2  then 'Seguridad\n\t\t\t\t\tif Id_Pregunta_Auditoria = 23 .AND. Cumple = .T. then\t\t\t\t\t\t\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\t\n\t\t\t\t\t\t\tesBasc = .T.\t\t\t\t\t\n\t\t\t\t\t\telse\n\t\t\t\t\t\t\tif esBasc = .F. .AND. Cumple = .T. .AND. Id_Pregunta_Auditoria <> 33 .AND. Id_Pregunta_Auditoria <> 39 then\n\t\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\n\t\t\t\t\t\t\tend if\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\tend if\n\t\t\t\n\t\t\tif Id_Proceso = 4 then 'Experiencia y mercado\n\t\t\t\t\tif condicion = \"Mayor\" then\n\t\t\t\t\t\tif cantidad >= valorCondicion .AND. cantidad > 0 then\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\n\t\t\t\t\t\tend if\n\t\t\t\t\t\telse\n\t\t\t\t\t\t\tif cantidad <= valorCondicion .AND. cantidad > 0 then\n\t\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\n\t\t\t\t\t\t\tend if\t\t\t\t\t\t\n\t\t\t\t\tend if\t\n\t\t\tend if\n\t\t\t\t\n\t\t\tif Cumple = .t. .AND. Id_Proceso = 1 then 'LEGAL\t\t\t\n\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - Puntos\t\t\t\t\t\t\t\n\t\t\tend if\t\n\t\t\t\n\t\t\t\n\t\t\t\n\tend while\n\t\n\t\t\tif kapital > 0 .AND. frecuenciaCS > 0 then\n\t\t\t\t\t\tcalculoCapacidadOperativa = (((valorCS * (periodoPago/frecuenciaCS))/kapital) * porcientoCompraMRG)\n\t\t\t\t\t\tif calculoCapacidadOperativa <= 10 then\n\t\t\t\t\t\t\tpuntacionRiesgoEntidad = puntacionRiesgoEntidad - 7\n\t\t\t\t\t\tend if\n\t\t\tend if\n\n\tsql = <<%sql%\n\tUPDATE Auditoria_2Parte Set valorRiesgo = :puntacionRiesgoEntidad WHERE Id_Auditoria = :Id_Auditoria\n\t%sql%\n\t\n\targsP.Add(\"puntacionRiesgoEntidad\", puntacionRiesgoEntidad)\n\n\tdim flagResult as l\n\tflag = cn.Execute(sql, argsP)\t\n\t\ncn.Close()  \n                         \nend function    \n\nfunction insertarPreguntasAuditoria2Parte22 as v (e as p)\n\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim argsP as sql::Arguments\n\targsP.Add(\"Id_Entidad\", 0)\t\n\t\n\tdim Id_Auditoria as n\n\tId_Auditoria = e.lastIdentityValue\n\t\n\tdim Id_Entidad as n\n\n\tdim rs as sql::ResultSet\n\tsql = <<%sql%\n\tselect Id_Entidad, Organizacion, Referencia, Id_ClasificacionEntidad, Id_TipoPotencialidad, DetalleCertificacionSeguros \n\tfrom N_Entidad \n\twhere Id_ClasificacionEntidad IN (3,4,5,6,7,8,9,10,11,19,32,33,35,30,46,22,16,17,11,41,1) \n\tAND Id_PaisEntidad = 173  \n\tAND Id_TipoPotencialidad IN (3)\n\tAND Id_TipoEntidad IN (1,2,9) \n\tORDER BY Organizacion\n\t%sql%\t\n\t\n\tcn.Execute(sql)\t\n\trs = cn.ResultSet\n\t\n\twhile rs.NextRow() \n\t\tId_Entidad = rs.Data(1)\n\t\t\n\t\targsP.Add(\"Id_Entidad\", Id_Entidad)\t\n\t\n\t\tsql = <<%sql%\n\t\tINSERT INTO Auditoria_2Parte (Id_TipoAuditoria, Estado, Id_Entidad) VALUES (1, 1, :Id_Entidad)\n\t\t%sql%\n\t\t\n\t\tcn.Execute(sql, argsP)\n\t\n\tend while\t\n\t\n   cn.Close()                     \nend function    \n\nfunction insertarPreguntasAuditoria2Parte as v (e as p)\n\n\tdim cn as sql::Connection\n\tcn.open(\"::Name::Nibble\")\n\t\n\tdim sql as c\n\tdim argsP as sql::Arguments\n\t\n\tdim Id_Auditoria as n\n\tId_Auditoria = e.lastIdentityValue 'e._currentRowDataNew.Id_Auditoria 'e.lastIdentityValue \n\t\n\tdim Id_Entidad as n\n\tId_Entidad = e.DataSubmitted.Id_Entidad 'e._currentRowDataNew.Id_Entidad 'e.DataSubmitted.Id_Entidad\n\t\n\targsP.Add(\"Id_Entidad\", Id_Entidad)\n\t\n\tdim rs as sql::ResultSet\n\tsql = <<%sql%\n\tSELECT  Id_ClasificacionEntidad\n\tFROM    N_Entidad\n\tWHERE   Id_Entidad = :Id_Entidad\n\t%sql%\t\n\t\n\tcn.Execute(sql, argsP)\t\n\trs = cn.ResultSet\n\n'\tdim Id_Clasificacion_Actividad as